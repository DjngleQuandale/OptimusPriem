part.CFrame = rightArm.CFrame * CFrame.new(1, 1, 1)
local function attachPartToPlayer(player)
    
    local character = player.Character or player.CharacterAdded:Wait()

    
    local torso = character:FindFirstChild("Torso") or character:FindFirstChild("UpperTorso")
    if not torso then
        warn("Torso not found!")
        return
    end

  
    local newPart = Instance.new("Part")
    newPart.Size = Vector3.new(2, 2, 2) 
    newPart.Color = Color3.new(1, 1, 1)
    newPart.Anchored = false
    newPart.CanCollide = false
    newPart.Massless = true
    newPart.Parent = character

 
    local specialMesh = Instance.new("SpecialMesh")
    specialMesh.MeshId = "rbxassetid://17454042970" 
    specialMesh.TextureId = "rbxassetid://17454048929"
    specialMesh.Scale = Vector3.new(0.0012, 0.0012, 0.0012) 
    specialMesh.Parent = newPart

  
    local weld = Instance.new("Weld")
    weld.Part0 = torso
    weld.Part1 = newPart
    weld.C0 = CFrame.new(0, 0.1, 0) 
    weld.Parent = newPart
end


local player = game.Players.LocalPlayer
if player then
    attachPartToPlayer(player)
end

local function attachPartToPlayer(player)
    
    local character = player.Character or player.CharacterAdded:Wait()

    
    local torso = character:FindFirstChild("Torso") or character:FindFirstChild("UpperTorso")
    if not torso then
        warn("Torso not found!")
        return
    end

  
    local newPart = Instance.new("Part")
    newPart.Size = Vector3.new(2, 2, 2) 
    newPart.Color = Color3.new(1, 1, 1)
    newPart.Anchored = false
    newPart.CanCollide = false
    newPart.Massless = true
    newPart.Parent = character

 
    local specialMesh = Instance.new("SpecialMesh")
    specialMesh.MeshId = "rbxassetid://17454909520" 
    specialMesh.TextureId = "rbxassetid://17454263229"
    specialMesh.Scale = Vector3.new(0.0015, 0.0010, 0.0013) 
    specialMesh.Parent = newPart

  
    local weld = Instance.new("Weld")
    weld.Part0 = torso
    weld.Part1 = newPart
    weld.C0 = CFrame.new(0, -1, 0.1) 
    weld.Parent = newPart
end


local player = game.Players.LocalPlayer
if player then
    attachPartToPlayer(player)
end

